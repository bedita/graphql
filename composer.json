{
    "name": "bedita/graphql",
    "type": "cakephp-plugin",
    "description": "BEdita/GraphQL plugin for BEdita",
    "keywords": ["graphql", "cakephp", "cake3", "plugin", "bedita"],
    "license": "LGPL",
    "support": {
        "source": "https://github.com/bedita/dev-tools",
        "issues": "https://github.com/bedita/dev-tools/issues"
    },
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "~3.5.0",
        "webonyx/graphql-php": "~0.10"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "BEdita\\GraphQL\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BEdita\\GraphQL\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
